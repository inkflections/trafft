function toggleVideo(state) {
    // if state == 'hide', hide. Else: show video
    let div = document.getElementById("popupVid");
    let iframe = div.getElementsByTagName("iframe")[0].contentWindow;
    div.style.display = state === 'hide' ? 'none' : 'block';
    func = state === 'hide' ? 'pauseVideo' : 'playVideo';
    iframe.postMessage('{"event":"command","func":"' + func + '","args":""}','*');
}

// Close the dropdown if the user clicks outside of it
window.onclick = function(event) {
    if (!event.target.matches('.dropdown-btn')) {
        let dropdowns = document.getElementsByClassName("w-dropdown-list")[0];
        let i;
        for (i = 0; i < dropdowns.length; i++) {
            let openDropdown = dropdowns[i];
            if (openDropdown.classList.contains('show')) {
                openDropdown.classList.remove('show');
            }
        }
        if (dropdowns.classList.contains('w--open')) {
            dropdowns.classList.remove('w--open');
        }
        if (dropdowns.style.display === "block") {
            dropdowns.style.display = "none";
        }
    }
}

