let coll = document.getElementsByClassName("collapsible");
let i;

for (i = 0; i < coll.length; i++) {
    coll[i].addEventListener("click", function() {
        this.classList.toggle("active");
        $("svg").find(".tab-icon").css({ filter: 'brightness(0) invert(1)' });

        let content = this.nextElementSibling;
        if (content.style.display === "block") {
            content.style.display = "none";
        } else {
            content.style.display = "block";
        }
    });
}

function prices () {
    let price = {
        // earlyBird: 5,
        basic: 12,
        pro: 24,
        expert: 48,
        type: 'billed monthly',
        perMonth: '/mo',
        crossedPrice: 17,
    };

    if (document.getElementById("monthly").checked) {
        // price.earlyBird = 5;
        price.basic = 12;
        price.pro = 24;
        price.expert = 48;
        price.type = 'billed monthly';
        price.perMonth = '/mo';
        price.crossedPriceBasic = 12;
        price.crossedPricePro = 24;
        price.crossedPriceExpert = 48;
    }

    if (document.getElementById("yearly").checked) {
        // price.earlyBird = 50;
        price.basic = 120;
        price.pro = 228;
        price.expert = 460;
        price.type = "billed yearly";
        price.perMonth = '/year';
        price.crossedPriceBasic = 120;
        price.crossedPricePro = 228;
        price.crossedPriceExpert = 460;
    }
    document.getElementsByClassName('price-num')[0].innerHTML = price.basic;
    // document.getElementsByClassName('price-num')[1].innerHTML = price.earlyBird;
    document.getElementsByClassName('price-num')[1].innerHTML = price.pro;
    document.getElementsByClassName('price-num')[2].innerHTML = price.expert;

